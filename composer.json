{
    "name": "specialtactics/laravel-api-boilerplate",
    "description": "An API boilerplate for Laravel",
    "keywords": [
        "laravel",
        "boilerplate",
        "api",
        "api boilerplate",
        "laravel api"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Max Snow",
            "email": "contact@maxsnow.me"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.5",
        "specialtactics/l5-api": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.9",
        "beyondcode/laravel-dump-server": "^1.4",
        "facade/ignition": "^2.7",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "laravel/telescope": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan jwt:secret"
        ],
        "test": "./vendor/bin/phpunit --colors=always -v --testdox",
        "lint": "./vendor/bin/phpcs",
        "docs": [
            "@php artisan ide-helper:generate --no-interaction"
        ],
        "build": "./env/build.sh"
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
